{
  "extends": "airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "no-underscore-dangle": [0],
    "class-methods-use-this": 0,
    "global-require": 0,
    "no-confusing-arrow": 0
  },
  "plugins": [
    "react"
  ]
}
